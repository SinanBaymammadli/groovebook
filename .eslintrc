{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier", "prettier/react"],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": false,
        "printWidth": 100
      }
    ],
    "camelcase": [
      2,
      {
        "properties": "never",
        "ignoreDestructuring": true
      }
    ]
  },
  "plugins": ["prettier"],
  "env": {
    "browser": true
  }
}
